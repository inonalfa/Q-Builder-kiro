{
  "name": "q-builder-server",
  "version": "1.0.0",
  "description": "Q-Builder Backend API Server",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "type-check": "tsc --noEmit",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:seed": "npx sequelize-cli db:seed:all",
    "db:reset": "npx sequelize-cli db:migrate:undo:all && npm run db:migrate && npm run db:seed",
    "db:view": "npx tsx src/utils/db-viewer.ts",
    "db:stats": "npx tsx src/utils/db-viewer.ts stats",
    "db:users": "npx tsx src/utils/db-viewer.ts users",
    "db:professions": "npx tsx src/utils/db-viewer.ts professions",
    "db:catalog": "npx tsx src/utils/db-viewer.ts catalog",
    "db:clients": "npx tsx src/utils/db-viewer.ts clients",
    "db:quotes": "npx tsx src/utils/db-viewer.ts quotes",
    "db:projects": "npx tsx src/utils/db-viewer.ts projects",
    "db:payments": "npx tsx src/utils/db-viewer.ts payments",
    "cache:cleanup": "npx tsx src/utils/cleanupCache.ts",
    "test:pdf": "npx tsx src/utils/testPDF.ts",
    "test:pdf-system": "npx tsx src/utils/testPDFSystem.ts"
  },
  "keywords": [
    "construction",
    "quotes",
    "projects",
    "api"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fontsource/noto-sans-hebrew": "^5.2.6",
    "axios": "^1.11.0",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "csv-parse": "^5.5.2",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "google-auth-library": "^10.1.0",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.9.7",
    "passport": "^0.7.0",
    "passport-apple": "^2.0.2",
    "passport-google-oauth20": "^2.0.0",
    "pdfkit": "^0.14.0",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "rate-limiter-flexible": "^4.0.1",
    "sequelize": "^6.35.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.10.0",
    "@types/node-cron": "^3.0.11",
    "@types/nodemailer": "^6.4.14",
    "@types/passport": "^1.0.17",
    "@types/passport-google-oauth20": "^2.0.16",
    "@types/pdfkit": "^0.12.12",
    "@types/pg": "^8.10.9",
    "sequelize-cli": "^6.6.2",
    "tsx": "^4.6.0",
    "typescript": "^5.3.2"
  }
}
